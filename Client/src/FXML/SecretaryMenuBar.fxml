<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SecretaryMenuBarController">
   <menus>
      <Menu mnemonicParsing="false" text="Navigation">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#previous" text="Previous" />
            <MenuItem mnemonicParsing="false" onAction="#main_menu" text="Main Menu" />
            <MenuItem mnemonicParsing="false" onAction="#quit" text="Quit" />
        </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Change Assignment">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#ChangeStudentAssignment" text="Student" />
            <MenuItem mnemonicParsing="false" onAction="#ChangeTeacherAssignment" text="Teacher" />
        </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Semester">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#CreateSemester" text="Create" />
            <MenuItem mnemonicParsing="false" onAction="#DefineClasses" text="Define Classes" />
            <MenuItem mnemonicParsing="false" onAction="#AttachCurseToClass" text="Attach curse to Class" />
        </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Connection">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#logout" text="Logout" />
        </items>
      </Menu>
   </menus>
</MenuBar>
