<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar fx:id="bar" prefHeight="32.0" prefWidth="331.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TeacherMenuBarController">
   <menus>
      <Menu mnemonicParsing="false" text="File">
        <items>
            <MenuItem mnemonicParsing="false" onAction="#message" text="Message" />
            <MenuItem mnemonicParsing="false" onAction="#exit" text="Exit" />
        </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Assignments">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#DefineAssignments" text="Define" />
            <MenuItem mnemonicParsing="false" onAction="#CheckAssignments" text="Check/Evaluate Submissions" />
            
        </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Connection">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#logout" text="Logout" />
        </items>
      </Menu>
      <Menu mnemonicParsing="false" text="View">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#ViewPersonalInfo" text="Personal information" />
        </items>
      </Menu>
   </menus>
</MenuBar>
