<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ViewSubmitionController">
   <children>
      <AnchorPane fx:id="AssignmentView" layoutX="-54.0" layoutY="3.0" prefHeight="357.0" prefWidth="1228.0">
         <children>
            <Label layoutX="180.0" layoutY="41.0" prefHeight="30.0" prefWidth="144.0" text="Submitions:" textFill="#4c6bc5">
               <font>
                  <Font name="Copperplate Gothic Bold" size="20.0" />
               </font>
            </Label>
            <TableView fx:id="SubmittionTable" layoutX="101.0" layoutY="71.0" prefHeight="155.0" prefWidth="1066.0">
               <columns>
                  <TableColumn fx:id="IDColum" prefWidth="54.0" text="ID" />
                  <TableColumn fx:id="NameColum" prefWidth="193.0" text="Assignment Name" />
                  <TableColumn fx:id="PublishDateColum" prefWidth="155.0" text="Publish Date" />
                  <TableColumn fx:id="DeadlineColum" prefWidth="162.0" text="DeadLine" />
                  <TableColumn fx:id="CourseCol" prefWidth="142.0" text="Course" />
                  <TableColumn fx:id="isLateColum" prefWidth="69.0" text="isLate" />
                  <TableColumn fx:id="GradeColum" prefWidth="92.0" text="Grade" />
                  <TableColumn fx:id="PathColum" prefWidth="198.0" text="File Path" />
               </columns>
            </TableView>
            <Button fx:id="DownloadBtn" layoutX="503.0" layoutY="253.0" mnemonicParsing="false" onAction="#ClickDownload" prefHeight="50.0" prefWidth="263.0" text="Download Submition" textFill="#4c6bc5">
               <font>
                  <Font name="Copperplate Gothic Bold" size="18.0" />
               </font>
               <effect>
                  <DropShadow />
               </effect>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
