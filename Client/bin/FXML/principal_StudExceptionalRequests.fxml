<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.StudentExceptionalRequest">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Exceptional requests">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="studTbl" layoutX="14.0" layoutY="83.0" prefHeight="326.0" prefWidth="325.0">
        <columns>
          <TableColumn fx:id="studIdCln" prefWidth="75.0" text="Student id" />
          <TableColumn fx:id="studNameCln" prefWidth="101.0" text="Student name" />
            <TableColumn fx:id="CorsCln" prefWidth="75.0" text="Course" />
            <TableColumn fx:id="requestCln" prefWidth="75.0" text="Requset" />
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="66.0" text="Students course changes" />
      <Button fx:id="aprvBtn" layoutX="14.0" layoutY="416.0" mnemonicParsing="false" onAction="#approve" text="Approve" />
      <Button fx:id="DisaprvBtn" layoutX="263.0" layoutY="416.0" mnemonicParsing="false" onAction="#disapprove" text="Disapprove" />
   </children>
</AnchorPane>
