<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TeacherExceptionalRequest">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Change teacher requests">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="excepTbl" layoutX="14.0" layoutY="83.0" prefHeight="326.0" prefWidth="364.0">
        <columns>
          <TableColumn fx:id="currTeacherCln" prefWidth="105.0" text="Current teacher" />
          <TableColumn fx:id="newTeacherCln" prefWidth="100.0" text="New teacher" />
            <TableColumn fx:id="CorsCln" prefWidth="81.0" text="Course" />
            <TableColumn fx:id="classCln" prefWidth="77.0" text="Class room" />
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="66.0" text="Teachers changes" />
      <Button fx:id="aprvBtn" layoutX="14.0" layoutY="416.0" mnemonicParsing="false" onAction="#approve" text="Approve" />
      <Button fx:id="DisaprvBtn" layoutX="302.0" layoutY="416.0" mnemonicParsing="false" onAction="#disapprove" text="Disapprove" />
   </children>
